#%Module
#
# cmake 3.15.2 (System gcc-7.4.0)
#
#
set version 3.15.2

proc ModulesHelp { } {
	global version
        puts stderr "\tSets up the environment for cmake $version (System gcc-7.4.0)\n"
        puts stderr "\t- CMAKE_ROOT"
        puts stderr "\t- PATH"
}

module-whatis "Open-source, cross-platform tools to build, test and package software"
module-whatis "https://cmake.org/"

set pkghome /cm/shared/apps/cmake/gcc/$version
setenv CMAKE_ROOT $pkghome
prepend-path PATH $pkghome/bin
unset pkghome

set MSG "cmake $version (System gcc-7.4.0-1ubuntu1~18.04.1)"

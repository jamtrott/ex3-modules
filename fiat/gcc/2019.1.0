#%Module
#
# FIAT 2019.1.0 (System python-3.7.4)
#
#
set version 2019.1.0

proc ModulesHelp { } {
	global version
        puts stderr "\tSets up the environment for FIAT $version (System python-3.7.4)\n"
        puts stderr "\t- FIAT_ROOT"
        puts stderr "\t- PYTHONPATH"
}

module-whatis "FInite element Automatic Tabulator"
module-whatis "https://bitbucket.org/fenics-project/fiat/"

set pkghome /cm/shared/apps/fiat/gcc/$version
setenv FIAT_ROOT $pkghome
prepend-path PYTHONPATH $pkghome/lib/python3.7/site-packages
unset pkghome

set MSG "FIAT $version (System python-3.7.4)"

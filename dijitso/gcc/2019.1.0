#%Module
#
# dijitso 2019.1.0 (System python-3.7.4)
#
#
set version 2019.1.0

proc ModulesHelp { } {
	global version
        puts stderr "\tSets up the environment for dijitso $version (System python-3.7.4)\n"
        puts stderr "\t- DIJITSO_ROOT"
        puts stderr "\t- PATH"
        puts stderr "\t- PYTHONPATH"
}

module-whatis "Distributed just-in-time compilation"
module-whatis "https://bitbucket.org/fenics-project/dijitso/"

set pkghome /cm/shared/apps/dijitso/gcc/$version
setenv DIJITSO_ROOT $pkghome
prepend-path PATH $pkghome/bin
prepend-path PYTHONPATH $pkghome/lib/python3.7/site-packages
unset pkghome

set MSG "dijitso $version (System python-3.7.4)"

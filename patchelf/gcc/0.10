#%Module
#
# patchelf 0.10 (System gcc-7.4.0)
#
#
set version 0.10

proc ModulesHelp { } {
	global version
        puts stderr "\tSets up the environment for patchelf $version (System gcc-7.4.0)\n"
        puts stderr "\t- PATH"
        puts stderr "\t- MANPATH"
}

module-whatis "sets up the environment for patchelf 0.10 (System gcc-7.4.0-1ubuntu1~18.04.1)"
module-whatis "https://github.com/NixOS/patchelf"

set pkghome /cm/shared/apps/patchelf/gcc/$version
prepend-path PATH $pkghome/bin
prepend-path MANPATH $pkghome/share/man
unset pkghome

set MSG "patchelf $version (System gcc-7.4.0-1ubuntu1~18.04.1)"

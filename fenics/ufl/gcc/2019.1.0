#%Module
#
# ufl 2019.1.0 (System python-3.7.4)
#
#
set version 2019.1.0

proc ModulesHelp { } {
	global version
        puts stderr "\tSets up the environment for ufl $version (System python-3.7.4)\n"
        puts stderr "\t- UFL_ROOT"
        puts stderr "\t- PYTHONPATH"
}

module-whatis "Unified Form Language for finite element variational forms"
module-whatis "https://bitbucket.org/fenics-project/ufl/"

set pkghome /cm/shared/apps/ufl/gcc/$version
setenv UFL_ROOT $pkghome
prepend-path PYTHONPATH $pkghome/lib/python3.7/site-packages
unset pkghome

set MSG "ufl $version (System python-3.7.4)"
